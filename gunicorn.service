[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=django
Group=www-data
WorkingDirectory=/home/django/matreshka_doc
ExecStart=/home/django/matreshka_doc/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          matreshka_doc.wsgi:application

[Install]
WantedBy=multi-user.target